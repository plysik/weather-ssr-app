{
  "name": "weather-ssr-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"pnpm run watch-client\" \"pnpm run watch-server\" \"pnpm run open:browser\"",
    "open:browser": "wait-on http://localhost:3000 && open-cli http://localhost:3000",
    "watch-client": "webpack --config webpack.client.ts --watch",
    "watch-server": "nodemon --watch src/server --exec \"pnpm run build:server && node build/server.js\"",
    "build:client": "webpack --config webpack.client.ts",
    "build:server": "webpack --config webpack.server.ts",
    "build": "pnpm run build:client && pnpm run build:server",
    "start": "node build/server.js",
    "biome:check": "biome check",
    "biome:fix": "biome fix",
    "check:types": "tsc --noEmit"
  },
